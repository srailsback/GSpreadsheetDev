<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="GSpreadsheetDev.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <applicationSettings>
    <GSpreadsheetDev.Properties.Settings>
      <setting name="GSpreadsheetDev_LarimerParcelInfo_PropertyInformation"
          serializeAs="String">
        <value>http://www.larimer.org/webservices/PropertyInformation.cfc</value>
      </setting>
    </GSpreadsheetDev.Properties.Settings>
  </applicationSettings>
  <appSettings>
    <!-- google account -->
    <add key="gAccount" value=""/>

    <!-- google password -->
    <add key="gPassword" value=""/>

    <!-- name of the servce, what ever you call it -->
    <add key="sheetService" value="FGPService"/>

    <!-- the name of the worksheet -->
    <add key="sheetTitle" value="FHGPParcels"/>

    <!-- target worksheet, zero based, 0 is first worksheet in the workbook, 1 is the 2nd worksheet -->
    <add key="worksheetNumber" value="2"/>

    <!-- column contains parcel numbers to query, zero-based, 0 is Column A, 2 is Column B -->
    <add key="parcelColumnNumber" value="1"/>
  </appSettings>
</configuration>